<h2>Log In</h2>

{{>flashMessages}}

<form method="POST" action="/process-login">
  <label for="email">
    <input type="email" name="email" placeholder="Enter your username here">
  </label>
  <br>

  <label for="loginPassword">
    <input type="password" name="loginPassword" placeholder="******">
  </label>
  <br>

  <button>Log In</button>
</form>